<%= simple_form_for(@asociado) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :facebook %>
    <%= f.input :twitter %>
    <%= f.input :pinterest %>
    <%= f.input :nombre %>
    <%= f.input :logo %>
    <%= f.input :descripcion %>
    <%= f.input :telefono %>
    <%= f.input :direccion %>
    <%= f.input :web %>
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
