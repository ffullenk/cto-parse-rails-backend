<%= simple_form_for @foto, :html =>{:multipart => true, :class => 'form-horizontal' } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :foto ,:as=> :file%>
    <%= f.input :descripcion %>
    <%= f.input :categoryId %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
